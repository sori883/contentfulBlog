    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "canvas": "^2.6.1",
    "graphql": "^16.6.0",
    "next": "13.0.0",
    "next-head-seo": "^0.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.3",
