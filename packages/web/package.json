    "babel-plugin-react-require": "^3.1.3",
    "chromatic": "^6.11.4",
    "date-fns": "^2.29.3",
    "next-sitemap": "^3.1.32",
    "next-transpile-modules": "^10.0.0",
    "npm-run-all": "^4.1.5",
